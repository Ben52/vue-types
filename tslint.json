{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
      "semicolon": [true, "never"],
      "quotemark": [true, "single"],
      "max-line-length": false,
      "trailing-comma": [true, "never"],
      "curly": false
    },
    "rulesDirectory": []
}
